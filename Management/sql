-- Table: cutomers.CUSTOMERS

DROP TABLE customers."CUSTOMERS";

CREATE TABLE customers."CUSTOMERS"
(
    id integer NOT NULL,
    name character(100) COLLATE pg_catalog."default" NOT NULL,
    country character(100) COLLATE pg_catalog."default" NOT NULL,
    city character(100) COLLATE pg_catalog."default" NOT NULL,
    status character(100) COLLATE pg_catalog."default" NOT NULL,
    CONSTRAINT "CUSTOMERS_pkey" PRIMARY KEY (id)
)

TABLESPACE pg_default;

ALTER TABLE customers."CUSTOMERS"
    OWNER to postgres;
    
